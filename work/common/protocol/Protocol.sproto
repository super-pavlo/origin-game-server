#队列信息
.QueueInfo {
    queueIndex 0 : integer                              #索引
    main 1 : boolean                                    #是否主队列
    finishTime 2 : integer                              #结束时间
    buildingIndex 3 : integer                           #对应建筑索引
    expiredTime 4 : integer                             #-1为永久
    timerId 5 : integer                                 #对应定时器id  
    type 6 : integer                                    #对应队列类型   1 建筑 2 训练
    armyType 7 : integer                                #对应兵种
    newArmyLevel 8 : integer                            #兵种等级
    armyNum 9 : integer                                 #兵种数量     
    oldArmyLevel 10 : integer                           #晋升才有用
    beginTime 11 : integer                              #开始时间
    technologyType 12 : integer                         #研究科技类型
    treatmentSoldiers 13 : *SoldierInfo(id)             #治疗士兵信息
    firstFinishTime 14 : integer                        #初始结束时间
    healSpeedMulti 15 : integer                         #治疗加成
    requestGuildHelp 16 : boolean                       #是否已发送过联盟求助
    .Items {
        itemId 0 : integer
        itemNum 1 : integer                             
    }
    produceItems 17 : *Items                            #铁匠铺正在锻造的材料列表
    completeItems 18 : *Items                           #完成的材料列表                           
}

#活动时间信息
.ActivityTimeInfo {
    activityId 0 : integer                          #活动id
    startTime 1 : integer                           #活动开始时间
    endTime 2 : integer                             #活动结束时间
    selfRank 3 : *integer                           #最强执政官个人排行信息
}

