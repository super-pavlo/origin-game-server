##############################################################################################################################

.package {
    type 0 : integer
    session 1 : integer
}

.Header {
    uid 0 : integer
    deviceType 1 : string                               #windows, ios, android
    clientVersion 2 : string
}

.ErrorMessage {
    errorCode 0 : integer                               #error code define at ErrorCode.lua
    errorMessage 1 : string                             #only for debug
}

.MessageContent {
    networkMessage 0 : binary                           #sproto encode binary
    error 1 : ErrorMessage
}

GateMessage 0 {
    request {
        content 0 : *MessageContent
        head 1 : Header
    }
    response {
        content 0 : *MessageContent
        head 1 : Header
    }
}

